                <assign variable="bulletInitialx" expression="tankx+x1"/>
                <assign variable="bulletInitialy" expression="tanky+y1"/>
                <comment text="&#1103;&#1084;&#1072;&#1088; &#1085;&#1101;&#1075;&#1101;&#1085; &#1199;&#1081;&#1083; &#1103;&#1074;&#1076;&#1072;&#1083; &#1073;&#1086;&#1083;&#1089;&#1086;&#1085; &#1101;&#1089;&#1101;&#1093;&#1080;&#1081;&#1075; &#1096;&#1072;&#1083;&#1075;&#1072;&#1085;&#1072;. &#1090;&#1072;&#1085;&#1082; &#1091;&#1089;&#1090;&#1089;&#1072;&#1085;, &#1075;&#1072;&#1079;&#1072;&#1088; &#1093;&#1199;&#1088;&#1089;&#1101;&#1085;, &#1093;&#1103;&#1079;&#1075;&#1072;&#1072;&#1088;&#1072;&#1072;&#1089; &#1076;&#1072;&#1074;&#1089;&#1072;&#1085;."/>
                <assign variable="col" expression="checkCollision(bulletinitialx, bulletinitialy, hitpoints, sand, col)"/>
            </while>
