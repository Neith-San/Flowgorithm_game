            </if>
            <comment text="&#1075;&#1072;&#1079;&#1072;&#1088; &#1079;&#1091;&#1088;&#1085;&#1072;"/>
            <home/>
            <turn expression="90" rotate="right"/>
            <comment text="&#1075;&#1072;&#1079;&#1072;&#1088; &#1085;&#1100; 12 &#1085;&#1199;&#1076;&#1090;&#1101;&#1081; &#1073;&#1072;&#1081;&#1085;&#1072;"/>
            <for variable="i" start="0" end="11" direction="inc" step="1">
                <comment text="&#1075;&#1072;&#1079;&#1088;&#1099;&#1085; &#1257;&#1085;&#1076;&#1257;&#1088; &#1085;&#1072;&#1084;&#1099;&#1075; &#1089;&#1072;&#1085;&#1072;&#1084;&#1089;&#1072;&#1088;&#1075;&#1199;&#1081;&#1075;&#1101;&#1101;&#1088; &#1089;&#1086;&#1085;&#1075;&#1086;&#1078; &#1199;&#1199;&#1089;&#1075;&#1101;&#1085;&#1101;"/>
                <assign variable="r" expression="random(2)"/>
                <comment text="&#1257;&#1084;&#1085;&#1257;&#1093; &#1085;&#1199;&#1076;&#1085;&#1101;&#1101;&#1089; &#1076;&#1101;&#1101;&#1096; &#1101;&#1089;&#1074;&#1101;&#1083; &#1076;&#1086;&#1086;&#1096; &#1073;&#1072;&#1081;&#1093;&#1099;&#1075; &#1089;&#1072;&#1085;&#1072;&#1084;&#1089;&#1072;&#1088;&#1075;&#1199;&#1081;&#1075;&#1101;&#1101;&#1088; &#1089;&#1086;&#1085;&#1075;&#1086;&#1085;&#1086;"/>
                <if expression="r == 1">
                    <then>
