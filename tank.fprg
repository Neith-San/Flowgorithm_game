            <comment text="&#1076;&#1072;&#1074;&#1090;&#1072;&#1083;&#1090; &#1073;&#1086;&#1083;&#1086;&#1085; &#1090;&#1199;&#1088; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1072;&#1093; &#1084;&#1101;&#1076;&#1101;&#1101;&#1083;&#1083;&#1199;&#1199;&#1076;&#1080;&#1081;&#1075; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1085;&#1072;"/>
            <declare name="h, i, r, j, temp, col" type="Integer" array="False" size=""/>
            <comment text="&#1090;&#1072;&#1085;&#1082;&#1085;&#1091;&#1091;&#1076;&#1099;&#1085; &#1073;&#1072;&#1081;&#1088;&#1096;&#1083;&#1099;&#1085; &#1084;&#1101;&#1076;&#1101;&#1101;&#1083;&#1083;&#1080;&#1081;&#1075; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1085;&#1072;"/>
            <declare name="tank1, tank2, tank1x, tank1y, tank2x, tank2y" type="Integer" array="False" size=""/>
            <comment text="&#1090;&#1072;&#1085;&#1082;&#1085;&#1091;&#1091;&#1076;&#1099;&#1085; &#1094;&#1086;&#1093;&#1080;&#1083;&#1090;&#1099;&#1085; &#1094;&#1101;&#1075;&#1199;&#1199;&#1076;&#1080;&#1081;&#1085; &#1084;&#1101;&#1076;&#1101;&#1101;&#1083;&#1083;&#1080;&#1081;&#1075; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1085;&#1072;"/>
            <declare name="hitpoints" type="Integer" array="True" size="9"/>
            <comment text="&#1093;&#1101;&#1088;&#1101;&#1074; &#1089;&#1072;&#1083;&#1093;&#1080;&#1090;&#1072;&#1081; &#1073;&#1086;&#1083; &#1089;&#1072;&#1083;&#1093;&#1080;&#1085;&#1099; &#1090;&#1091;&#1093;&#1072;&#1081; &#1084;&#1101;&#1076;&#1101;&#1101;&#1083;&#1083;&#1080;&#1081;&#1075; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1085;&#1072;"/>
            <declare name="salhi" type="Real" array="False" size=""/>
            <assign variable="h" expression="0"/>
            <assign variable="col" expression="0"/>
            <assign variable="hitpoints[0]" expression="0"/>
            <comment text="2 &#1090;&#1072;&#1085;&#1082;&#1085;&#1099; &#1073;&#1072;&#1081;&#1088;&#1096;&#1083;&#1091;&#1091;&#1076;&#1099;&#1085; &#1084;&#1101;&#1076;&#1101;&#1101;&#1083;&#1083;&#1080;&#1081;&#1075; &#1093;&#1072;&#1076;&#1075;&#1072;&#1083;&#1085;&#1072;. &#1093;&#1086;&#1086;&#1088;&#1086;&#1085;&#1076;&#1086;&#1086; &#1073;&#1072;&#1075;&#1072;&#1076;&#1072;&#1072; 6 &#1085;&#1199;&#1076;&#1085;&#1080;&#1081; &#1079;&#1072;&#1081;&#1090;&#1072;&#1081; &#1073;&#1072;&#1081;&#1085;&#1072;."/>
            <assign variable="tank1" expression="random(6)"/>
