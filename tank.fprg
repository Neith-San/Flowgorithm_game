            <comment text="salhi"/>
            <assign variable="salhi" expression="0"/>
            <comment text="&#1093;&#1101;&#1088;&#1101;&#1074; &#1089;&#1072;&#1083;&#1093;&#1080;&#1090;&#1072;&#1081; &#1073;&#1072;&#1081;&#1083;&#1075;&#1072;&#1093; &#1073;&#1086;&#1083; &#1089;&#1072;&#1083;&#1093;&#1080;&#1081;&#1075; &#1089;&#1072;&#1085;&#1072;&#1084;&#1089;&#1072;&#1088;&#1075;&#1199;&#1081;&#1075;&#1101;&#1101;&#1088; &#1199;&#1199;&#1089;&#1075;&#1101;&#1085;&#1101;"/>
            <if expression="salhilah==True">
                <then>
                    <if expression="Random(2)==0">
                        <then>
